<html>
	<body>
		<table>
			<tr>
				<th>Expense Report ID</th>
				<th>Description</th>
				<th>Currency</th>
				<th>Amount</th>
				<th>Conversion Rate</th>
				<th>Amount in INR</th>
			</tr>
			<%@expense_report.consolidated_expenses.each do|expense|%>
				<tr>
					<td><%=expense["report_id"]%></td>
					<td></td>
					<td><%=expense["currency"]%></td>
					<td><%=expense["amount"]%></td>
					<td><%=expense["conversion_rate"]%></td>
					<td><%=expense["local_currency_amount"]%></td>
				</tr>
			<% end %>
		</table>	
	</body>
</html>
